    {
      "parameters": {
        "operation": "executeQuery",
        "query": "/* Engelske originalrækker pr. site i den ønskede by */\nWITH en AS (\n    SELECT\n        site_id,\n        city_id,\n        updated_at AS en_updated_at\n    FROM site_translations\n    WHERE language_code = 'en'\n      -- AND city_id = 'rome'              -- evt. by-filter\n)\n\n/* Find alle sprog for byen i city_translations,\n   og check om site-oversættelsen mangler eller er forældet */\nSELECT DISTINCT\n       en.site_id,\n       en.city_id,\n       ct.language_code            AS missing_or_outdated_lang\nFROM       en\nJOIN       city_translations ct    -- alle sprog der findes for byen\n       ON  ct.city_id = en.city_id\n       AND ct.language_code <> 'en'  -- udeluk engelsk\nLEFT JOIN  site_translations st     -- tilstedeværende site-oversættelser\n       ON  st.site_id       = en.site_id\n       AND st.city_id       = en.city_id\n       AND st.language_code = ct.language_code\nWHERE  st.updated_at IS NULL        -- mangler helt\n    OR st.updated_at < en.en_updated_at;  -- eller ældre end engelsk\n",
        "options": {}
      },
