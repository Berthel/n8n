        "assignments": {
          "assignments": [
            {
              "id": "2382d99d-9609-458d-8ebf-12f39d93bfc0",
              "name": "links",
              "value": "={{ $json.links }}",
              "type": "object"
            },
            {
              "id": "4641756d-ed54-4441-a286-b0fb05a68564",
              "name": "commons",
              "value": "={{ $json.commons }}",
              "type": "string"
            }
          ]
        },
        "options": {}
      },
      "type": "n8n-nodes-base.set",
      "typeVersion": 3.4,
      "position": [
        1856,
        -660
      ],
      "id": "0896ea13-ac4f-4603-a4bb-2a8a1cd0b7fa",
