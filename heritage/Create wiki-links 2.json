                "type": "object"
              },
              {
                "id": "4641756d-ed54-4441-a286-b0fb05a68564",
                "name": "commons",
                "value": "={{ $json.commons }}",
                "type": "string"
              }
            ]
          },
          "options": {}
        },
        "type": "n8n-nodes-base.set",
        "typeVersion": 3.4,
